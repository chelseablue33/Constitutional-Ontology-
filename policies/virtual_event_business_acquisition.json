{
  "policy_id": "virtual_event_business_acquisition",
  "policy_version": "1.0.0",
  "created": "2023-10-15",
  "description": "Policy to govern the virtual event aimed at educating participants on business acquisition strategies for wealth creation, facilitated by Codie Sanchez and team over a three-day interactive session.",
  "dials": {
    "autonomy": {
      "level": "L2",
      "label": "Moderate Autonomy",
      "description": "Participants have guided autonomy to explore business acquisition strategies."
    },
    "personalization": {
      "level": "L2",
      "label": "Moderate Personalization",
      "description": "Content is tailored to participant interests in business acquisition."
    },
    "memory": {
      "level": "L1",
      "label": "Minimal Memory",
      "description": "Limited retention of participant interactions to ensure privacy."
    },
    "inter_agent": {
      "level": "L1",
      "label": "Minimal Inter-Agent Communication",
      "description": "Limited communication between agents to maintain focus on event objectives."
    }
  },
  "gates": {
    "U-I": {
      "name": "User Inbound",
      "direction": "user_to_agent",
      "allow": [
        "register_event",
        "submit_questions"
      ],
      "controls": [
        "access_control"
      ],
      "deny": []
    },
    "U-O": {
      "name": "User Outbound",
      "direction": "agent_to_user",
      "allow": [
        "provide_event_details",
        "send_confirmation"
      ],
      "controls": [
        "redaction_control"
      ],
      "deny": []
    },
    "S-I": {
      "name": "System Inbound",
      "direction": "system_to_agent",
      "allow": [
        "fetch_event_data"
      ],
      "controls": [
        "security_check"
      ],
      "deny": []
    },
    "S-O": {
      "name": "System Outbound",
      "direction": "agent_to_system",
      "allow": [
        "update_event_status"
      ],
      "controls": [
        "security_check"
      ],
      "deny": []
    },
    "M-I": {
      "name": "Memory Inbound",
      "direction": "memory_to_agent",
      "allow": [
        "store_participant_data"
      ],
      "controls": [],
      "deny": []
    },
    "M-O": {
      "name": "Memory Outbound",
      "direction": "agent_to_memory",
      "allow": [],
      "controls": [
        "retention_policy"
      ],
      "deny": [
        "export_participant_data"
      ]
    },
    "A-I": {
      "name": "Agent Inbound",
      "direction": "other_agent_to_agent",
      "allow": [],
      "controls": [],
      "deny": []
    },
    "A-O": {
      "name": "Agent Outbound",
      "direction": "agent_to_other_agent",
      "allow": [],
      "controls": [],
      "deny": []
    }
  },
  "overlays_enabled": [],
  "overlays": {},
  "rules": [
    {
      "rule_id": "R-001",
      "baseline": false,
      "enabled": true,
      "description": "Ensure event is live and virtual with interactive components.",
      "severity": "allow",
      "policy_clause_ref": "ยง1.1",
      "applies_to_gate": "U-I",
      "applies_to_control": "access_control"
    },
    {
      "rule_id": "R-002",
      "baseline": false,
      "enabled": true,
      "description": "Facilitate a three-day event with Codie Sanchez and team.",
      "severity": "allow",
      "policy_clause_ref": "ยง1.2",
      "applies_to_gate": "S-I",
      "applies_to_control": "security_check"
    },
    {
      "rule_id": "R-003",
      "baseline": false,
      "enabled": true,
      "description": "Focus on business acquisition for wealth creation.",
      "severity": "allow",
      "policy_clause_ref": "ยง1.3",
      "applies_to_gate": "U-O",
      "applies_to_control": "redaction_control"
    }
  ]
}